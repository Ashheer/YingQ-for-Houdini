{
    "path":"$LibPath/YingQ-for-Houdini",
    "env": [
       {
          "YingQLib": "$LibPath/YingQ-for-Houdini",
       },
       {
         "HOUDINI_SCRIPT_PATH": {
             "method": "prepend",
             "value": [
                 "$YingQLib/scripts",
             ]
         }
       },
       {
          "HOUDINI_TEMP_DIR" : "$YingQLib/houdini_temp"
       },
       {
         "HOUDINI_DSO_PATH": {
            "method": "prepend",
            "value": [
               "$YingQLib/dso"
            ]
         }
      },
       {
          "HOUDINI_OTLSCAN_PATH": {
             "method": "append",
             "value": [
                "$YingQLib/otls"
             ]
          }
       },
       {
          "HOUDINI_TOOLBAR_PATH": {
             "method": "append",
             "value": [
                "$YingQLib/toolbar"
             ]
          }
       },
       {
          "HOUDINI_DESK_PATH": {
             "method": "append",
             "value": [
                "$YingQLib/desktop"
             ]
          }
       },
       {
          "HOUDINI_PYTHON_PANEL_PATH": {
             "method": "append",
             "value": [
                "$YingQLib/python_panels"
             ]
          }
       },
       {
         "HOUDINI_VEX_PATH": {
             "method": "append", 
             "value": [
                 "$YingQLib/vex/^",
                 "$YingQLib/vex/include",
                 "$YingQLib/vex/include/child",
                 "$YingQLib/vex/include/child/element",
                 "$YingQLib/vex/include/child/element/basic",
                 "$YingQLib/vex/include/child/element/basic/deep",
             ]
         }
       },
    ]
 }