{
    "path":"$LibPath/YingQ-for-Houdini",
    "env": [
       {
          "YingQLib": "$LibPath/YingQ-for-Houdini",
       },
       {
         "HOUDINI_SCRIPT_PATH": {
             "method": "prepend",
             "value": [
                 "$YingQLib/scripts",
             ]
         }
       },
       {
         "PYTHONPATH": {
            "method": "append",
            "value": [
               "$YingQLib/scripts/python"
            ]
         }
       },  
       {
          "HOUDINI_TEMP_DIR" : "$YingQLib/houdini_temp"
       },
       {
         "HOUDINI_DSO_PATH": {
            "method": "prepend", 
            "value": [
               "$YingQLib/dso"
            ]
         }
      },
       {
          "HOUDINI_OTLSCAN_PATH": {
             "method": "append",
             "value": [
                "$YingQLib/otls/Geo/Building",
                "$YingQLib/otls/Geo/Cable",
                "$YingQLib/otls/Geo/Curve",
                "$YingQLib/otls/Geo/HardSurface",
                "$YingQLib/otls/Geo/Layout",
                "$YingQLib/otls/Geo/Terrain",
                "$YingQLib/otls/Geo/WFC",
                "$YingQLib/otls/Group",
                "$YingQLib/otls/HDK",
                "$YingQLib/otls/Operation",
                "$YingQLib/otls/Scripts",
                "$YingQLib/otls/Subdivide",
                "$YingQLib/otls/Texture",
                "$YingQLib/otls/Test"
             ]
          }
       },
       {
          "HOUDINI_TOOLBAR_PATH": {
             "method": "append",
             "value": [
                "$YingQLib/toolbar"
             ]
          }
       },
       {
          "HOUDINI_DESK_PATH": {
             "method": "append",
             "value": [
                "$YingQLib/desktop"
             ]
          }
       },
       {
          "HOUDINI_PYTHON_PANEL_PATH": {
             "method": "append",
             "value": [
                "$YingQLib/python_panels"
             ]
          }
       },
       {
         "HOUDINI_VEX_PATH": {
             "method": "append", 
             "value": [
                 "$YingQLib/vex/^",
                 "$YingQLib/vex/include",
                 "$YingQLib/vex/include/child",
                 "$YingQLib/vex/include/child/element",
                 "$YingQLib/vex/include/child/element/basic",
                 "$YingQLib/vex/include/child/element/basic/deep",
             ]
         }
       },
    ]
 }