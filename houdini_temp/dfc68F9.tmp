Content-Disposition: attachment; filename="hdaroot/initialize_point_attributes.parm"
Content-Type: text/plain

{
version 0.8
python	[ 0	locks=0 ]	(	"node = hou.pwd()
geo = node.geometry()
geo2 = node.inputs()[1].geometry()

attribs = hou.evalParm(\"../attributes\").split(' ')
for attrib in attribs:
    ptattrib = geo2.findPointAttrib(attrib)
    if ptattrib and not geo.findPointAttrib(attrib):
        newattrib = geo.addAttrib(hou.attribType.Point, attrib, ptattrib.defaultValue(), create_local_variable=False)
        newattrib.setOption(\"type\", ptattrib.option(\"type\"))"	)
}

