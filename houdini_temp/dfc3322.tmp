kernelname	[ 0	locks=0 ]	(	relax	)
usecode	[ 0	locks=0 ]	(	"on"	)
kernelfile	[ 0	locks=0 ]	(	""	)
kernelcode	[ 0	locks=0 ]	(	"kernel void relax (
    int P_length,
    global float *P,
    int Pcopy_length,
    global float *Pcopy,
    int groupweight_length, 
    global float * groupweight,
    int ptlist_length, 
    global int *ptlist_index,
    global int *ptlist
)
{
    int idx = get_global_id ( 0 );
    if ( idx >= P_length )
        return;
    
    int nstart = ptlist_index [ idx ];
    int nend = ptlist_index [ idx + 1 ];
    
    float3 p = 0;
    int count = 0;
    for ( int i = nstart; i < nend; ++i )
    {
        p.x += P [ ptlist [ i ] * 3 ];
        p.y += P [ ptlist [ i ] * 3 + 1 ];
        p.z += P [ ptlist [ i ] * 3 + 2 ];
        ++count;
    }
    
    if ( groupweight [ idx ] > 0 )
    {
        Pcopy [ idx * 3 ] = p.x / count;
        Pcopy [ idx * 3 + 1 ] = p.y / count;
        Pcopy [ idx * 3 + 2 ] = p.z / count;
    }
}

kernel void writeBackToP (
    int P_length,
    global float *P,
    int Pcopy_length,
    global float *Pcopy,
    int groupweight_length, 
    global float * groupweight,
    int ptlist_length, 
    global int *ptlist_index,
    global int *ptlist
)
{
    int idx = get_global_id ( 0 );
    if ( idx >= P_length )
        return;
        
    if ( groupweight [ idx ] > 0 )
    {
        P [ idx * 3 ] = Pcopy [ idx * 3 ];
        P [ idx * 3 + 1 ] = Pcopy [ idx * 3 + 1 ];
        P [ idx * 3 + 2 ] = Pcopy [ idx * 3 + 2 ];
    }
}"	)
