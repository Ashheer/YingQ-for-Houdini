        p.x += ( Pcopy [ ptlist [ i ] * 3 ] - Pcopy [ idx * 3 ] ) * m;
        p.y += ( Pcopy [ ptlist [ i ] * 3 + 1 ] - Pcopy [ idx * 3 + 1 ] ) * m;
        p.z += ( Pcopy [ ptlist [ i ] * 3 + 2 ] - Pcopy [ idx * 3 + 2 ] ) * m;
    }
    
    P [ idx * 3 ] = Pcopy [ idx * 3 ] + groupweight [ idx ] * p.x * mu;
    P [ idx * 3 + 1 ] = Pcopy [ idx * 3 + 1 ] + groupweight [ idx ] * p.y * mu;
    P [ idx * 3 + 2 ] = Pcopy [ idx * 3 + 2 ] + groupweight [ idx ] * p.z * mu;
