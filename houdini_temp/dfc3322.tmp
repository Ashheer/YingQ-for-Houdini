--HOUDINIMIMEBOUNDARY0xD3ADD339-0x00000F49-0x56B122C9-0x00000001HOUDINIMIMEBOUNDARY
Content-Disposition: attachment; filename="hdaroot/opencl_smooth.chn"
Content-Type: text/plain

{
    channel bindings5_fval {
      lefttype = extend
      righttype = extend
      flags = 0
      segment { length = 0 expr = "{
    float l = ch(\"../lambda\");
    if(ch(\"../algorithm\") == 0)
    {
        float passband = ch(\"../frequency\");
        if (passband < 1e-5)
        {
            float sqrt2_5 = 0.6324555320336758664;
            l = sqrt2_5;
        }
        else
        {
            float discriminant = 4.0 * passband * passband - 24.0 * passband + 40.0;
            l = 2.0 * passband - sqrt(discriminant);
            l /= (6.0 * passband - 10.0);
        }
    }
    
    return l;
}" }
    }
    channel bindings6_fval {
      lefttype = extend
      righttype = extend
      flags = 0
      segment { length = 0 expr = "{
    float u = ch(\"../mu\");
    if(ch(\"../algorithm\") == 0)
    {
        float passband = ch(\"../frequency\");
        if (passband < 1e-5)
        {
            float sqrt2_5 = 0.6324555320336758664;
            u = -sqrt2_5;
        }
        else
        {
            float discriminant = 4.0 * passband * passband - 24.0 * passband + 40.0;
            float l = 2.0 * passband - sqrt(discriminant);
            l /= (6.0 * passband - 10.0);
            
            u = l / (passband * l - 1.0);
        }
    }
    
    return u;
}" }
    }
    channel iterations {
      lefttype = extend
      righttype = extend
      default = 100
      flags = 0
      segment { length = 0 value = 100 100 expr = ch(\"../iterations\") }
    }
  }

