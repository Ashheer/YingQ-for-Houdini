kernelcode	[ 0	locks=0 ]	(	"kernel void smoothPositive (
    int P_length,
    global float *P,
    int Pcopy_length,
    global float *Pcopy,
    int groupweight_length, 
    global float * groupweight,
    int ptlist_length, 
    global int *ptlist_index,
    global int *ptlist,
    float lambda,
    float mu
)
{
    int idx = get_global_id ( 0 );
    if ( idx >= P_length )
        return;
    
    int nstart = ptlist_index [ idx ];
    int nend = ptlist_index [ idx + 1 ];
    
    int count = nend - nstart;
    float m = 1.0 / count;
    
    float3 p = 0;
    for ( int i = nstart; i < nend; ++i )
    {
        p.x += ( P [ ptlist [ i ] * 3 ] - P [ idx * 3 ] ) * m;
        p.y += ( P [ ptlist [ i ] * 3 + 1 ] - P [ idx * 3 + 1 ] ) * m;
        p.z += ( P [ ptlist [ i ] * 3 + 2 ] - P [ idx * 3 + 2 ] ) * m;
    }
    
    Pcopy [ idx * 3 ] = P [ idx * 3 ] + groupweight [ idx ] * p.x * lambda;
    Pcopy [ idx * 3 + 1 ] = P [ idx * 3 + 1 ] + groupweight [ idx ] * p.y * lambda;
    Pcopy [ idx * 3 + 2 ] = P [ idx * 3 + 2 ] + groupweight [ idx ] * p.z * lambda;
}

