--HOUDINIMIMEBOUNDARY0xD3ADD339-0x00000F49-0x56B122C9-0x00000001HOUDINIMIMEBOUNDARY
Content-Disposition: attachment; filename="hdaroot/attribwrangle_smooth_positive.chn"
Content-Type: text/plain

{
    channel maxdist {
      lefttype = extend
      righttype = extend
      flags = 0
      segment { length = 0 expr = "2 * max(max(bbox(-1, D_XSIZE), bbox(-1, D_YSIZE)), bbox(-1, D_ZSIZE))" }
    }
    channel lambda {
      lefttype = extend
      righttype = extend
      flags = 0
      segment { length = 0 expr = "{
    float l = ch(\"../lambda\");
    if(ch(\"../algorithm\") == 0)
    {
        float passband = ch(\"../frequency\");
        if (passband < 1e-5)
        {
            float sqrt2_5 = 0.6324555320336758664;
            l = sqrt2_5;
        }
        else
        {
            float discriminant = 4.0 * passband * passband - 24.0 * passband + 40.0;
            l = 2.0 * passband - sqrt(discriminant);
            l /= (6.0 * passband - 10.0);
        }
    }
    
    return l;
}" }
    }
  }

