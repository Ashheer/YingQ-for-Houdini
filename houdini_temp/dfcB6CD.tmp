#include \"Complex.vfl\"
// create parameters of our special arc-length curve
float k = chi(\"Wind_number\");
float r = chf(\"Curvature\");
float q = pow(r,2*k-1);
float m = 2*k-1;
float a = 1/(2*k-1);
float b = 2*pow(r,k)/k;
float fac = 1/(r+q);

// read coordinates
float c = @P.x;
float s = @P.y;

// interprete as complex number and take complex exponents
vector2 zm = cpow(set(c,s),(int)m);
vector2 zk = cpow(set(c,s),(int)k);

// parametrize the curve
@P.x = fac*(r*c + a*q*zm.x);
@P.y = fac*b*zk.y;
@P.z = fac*(r*s - a*q*zm.y);"	)
