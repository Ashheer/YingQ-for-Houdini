Content-Type: text/plain

{
version 0.8
folder0	[ 0	locks=0 ]	(	1	1	)
group	[ 0	locks=0 ]	(	origprims_subdivide	)
grouptype	[ 0	locks=0 ]	(	"guess"	)
class	[ 0	locks=0 ]	(	"point"	)
vex_numcount	[ 0	locks=0 ]	(	10	)
vex_threadjobsize	[ 0	locks=0 ]	(	1024	)
snippet	[ 0	locks=0 ]	(	"string groupname = chs(\"groupname\");
string newgroupname = chs(\"newgroupname\");

int pts [ ] = neighbours ( 0, @ptnum );
foreach ( int pt; pts )
{
    int prs [ ] = { };
    
    int firsthedge = pointhedge ( 0, @ptnum, pt );
    int hedge = firsthedge;
    
    do
    {
        int pr = hedge_prim ( 0, hedge );
        if ( pr != -1 && find ( prs, pr ) < 0 )
            append ( prs, pr );
        
        hedge = hedge_nextequiv ( 0, hedge );
    }
    while ( hedge != firsthedge );
    
    int primcount = 0;
    foreach ( int pr; prs )
    {
        int isclosed = primintrinsic ( 0, \"closed\", pr );
        if ( isclosed && inprimgroup ( 0, groupname, pr ) )
            primcount += 1;
    }
    
    if ( primcount == 1 )
        setedgegroup ( 0, newgroupname, @ptnum, pt, 1 );
}"	)
