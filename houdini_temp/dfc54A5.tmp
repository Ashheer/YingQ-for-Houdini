--HOUDINIMIMEBOUNDARY0xD3ADD339-0x00000F49-0x56B122C9-0x00000001HOUDINIMIMEBOUNDARY
Content-Disposition: attachment; filename="hdaroot/create_new_edge_primitives.parm"
Content-Type: text/plain

{
version 0.8
folder0	[ 0	locks=0 ]	(	1	1	)
group	[ 0	locks=0 ]	(	nearbyprims_subdivide	)
grouptype	[ 0	locks=0 ]	(	"guess"	)
class	[ 0	locks=0 ]	(	"primitive"	)
vex_numcount	[ 0	locks=0 ]	(	10	)
vex_threadjobsize	[ 0	locks=0 ]	(	1024	)
snippet	[ 0	locks=0 ]	(	"int primindex = i@sourceprim_index;
int pts [ ] = primpoints ( 0, @primnum );
int vtx [ ] = primvertices ( 1, primindex );

int count = len ( pts );

int isclosed = primintrinsic ( 1, \"closed\", primindex );
if ( isclosed )
{
    append ( pts, pts [ 0 ] );
    append ( vtx, vtx [ 0 ] );
}
else
    count -= 1;
    
int newpts [ ] = pts;
int newvtx [ ] = vtx;

int vertexindices [ ] = { };
int weightindices [ ] = { };

int currentindex = 0;
for ( int i = 0; i < count; ++i )
{
    currentindex += 1;
    
    string edgepts = sprintf ( \"%s %s\", min ( pts [ i ], pts [ i + 1 ] ), max ( pts [ i ], pts [ i + 1 ] ) );
    int indices [ ] = findattribval ( 0, \"point\", \"edgepts\", edgepts );
    int indexcount = len ( indices );
    if ( indexcount > 0 )
    {
        int vtxindices [ ] = { };
        foreach ( int index; indices )
        {
            append ( vtxindices, -1 );
            append ( vertexindices, i );
            append ( weightindices, index );
        }
        
        int sourcept_indices [ ] = point ( 0, \"sourcept_indices\", indices [ 0 ] );
        if ( pts [ i ] != sourcept_indices [ 0 ] )
            indices = reverse ( indices );
            
        insert ( newpts, currentindex, indices );
        insert ( newvtx, currentindex, vtxindices );
        currentindex += indexcount;
    }
}

if ( isclosed )
{
    removeindex ( newpts, -1 );
    removeindex ( newvtx, -1 );
}

string typename = \"poly\";
if ( !isclosed )
    typename = \"polyline\";

removeprim ( 0, @primnum, 0 );

int pr = -1;
int newvertices [ ] = { };
addprim ( pr, 0, typename, newpts, newvertices );

int primindices [ ] = array ( primindex );
float primweights [ ] = array ( 1.0 );

setprimattrib ( 0, \"sourceprim_index\", pr, primindex );
setprimattrib ( 0, \"sourceprim_indices\", pr, primindices );
setprimattrib ( 0, \"sourceprim_weights\", pr, primweights );
setprimgroup ( 0, \"nearbyprims_subdivide\", pr, 1 );

int index = 0;
count = len ( newvtx );
for ( int i = 0; i < count; ++i )
{
    int sourcevtx_indices [ ] = { };
    float sourcevtx_weights [ ] = { };
    int sourcevtx_groupindices [ ] = { };
    
    if ( newvtx [ i ] != -1 )
    {
        sourcevtx_indices = array ( newvtx [ i ] );
        sourcevtx_weights = array ( 1.0 );
        sourcevtx_groupindices = sourcevtx_indices;
    }
    else
    {
        sourcevtx_indices = array ( vtx [ vertexindices [ index ] ], vtx [ vertexindices [ index ] + 1 ] );
        float w [ ] = point ( 0, \"sourcept_weights\", weightindices [ index ] );
        sourcevtx_weights = w;
        sourcevtx_groupindices = array ( -1, -1 );
        
        index += 1;
    }
    
    setvertexgroup ( 0, \"newvtx_subdivide\", newvertices [ i ], -1, 1 );
    setvertexattrib ( 0, \"sourcevtx_indices\", newvertices [ i ], -1, sourcevtx_indices );
    setvertexattrib ( 0, \"sourcevtx_weights\", newvertices [ i ], -1, sourcevtx_weights );
    setvertexattrib ( 0, \"sourcevtx_groupindices\", newvertices [ i ], -1, sourcevtx_groupindices );
}"	)
exportlist	[ 0	locks=0 ]	(	*	)
vex_strict	[ 0	locks=0 ]	(	"off"	)
autobind	[ 0	locks=0 ]	(	"on"	)
bindings	[ 0	locks=0 ]	(	0	)
groupautobind	[ 0	locks=0 ]	(	"on"	)
groupbindings	[ 0	locks=0 ]	(	0	)
vex_cwdpath	[ 0	locks=0 ]	(	.	)
vex_outputmask	[ 0	locks=0 ]	(	*	)
vex_updatenmls	[ 0	locks=0 ]	(	"off"	)
vex_matchattrib	[ 0	locks=0 ]	(	id	)
vex_inplace	[ 0	locks=0 ]	(	"off"	)
vex_selectiongroup	[ 0	locks=0 ]	(	edgeprims_edgedivide	)
vex_precision	[ 0	locks=0 ]	(	auto	)
folder1	[ 0	locks=0 ]	(	0	0	)
}

