{
version 0.8
folder0	[ 0	locks=0 ]	(	1	1	)
group	[ 0	locks=0 ]	(	origpts_subdivide	)
grouptype	[ 0	locks=0 ]	(	"guess"	)
class	[ 0	locks=0 ]	(	"point"	)
vex_numcount	[ 0	locks=0 ]	(	10	)
vex_threadjobsize	[ 0	locks=0 ]	(	1024	)
snippet	[ 0	locks=0 ]	(	"int edgepts [ ] = i[]@edgeneighbours;

int facepts [ ] = { };
int prs [ ] = pointprims ( 0, @ptnum );
foreach ( int pr; prs )
{
    int pts [ ] = primpoints ( 0, pr );
    foreach ( int pt; pts )
        if ( pt != @ptnum && find ( edgepts, pt ) < 0 && find ( facepts, pt ) < 0 && inpointgroup ( 0, \"facepts_subdivide\", pt ) )
            append ( facepts, pt );
}

i[]@facepts = facepts;"	)
