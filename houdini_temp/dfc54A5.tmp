{
version 0.8
folder0	[ 0	locks=0 ]	(	1	1	)
group	[ 0	locks=0 ]	(	edgepts_subdivide	)
grouptype	[ 0	locks=0 ]	(	"points"	)
class	[ 0	locks=0 ]	(	"point"	)
vex_numcount	[ 0	locks=0 ]	(	10	)
vex_threadjobsize	[ 0	locks=0 ]	(	1024	)
snippet	[ 0	locks=0 ]	(	"if ( !i@isclosed )
{
    int edgepts [ ] = i[]@edgeneighbours;
    
    float controlweight = 6.0 / 8;
    float edgeweight = 1.0 / 8;
    
    int indices [ ] = array ( @ptnum );
    float weights [ ] = array ( controlweight );
    
    foreach ( int pt; edgepts )
    {
        append ( indices, pt );
        append ( weights, edgeweight );
    }
    
    i[]@sourcept_indices = indices;
    f[]@sourcept_weights = weights;
    
    i@group_curveedgepts_subdivide = 1;
}"	)
exportlist	[ 0	locks=0 ]	(	*	)
vex_strict	[ 0	locks=0 ]	(	"off"	)
autobind	[ 0	locks=0 ]	(	"on"	)
