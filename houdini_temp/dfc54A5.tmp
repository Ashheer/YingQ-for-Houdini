Content-Disposition: attachment; filename="hdaroot/create_new_edge_points.parm"
Content-Type: text/plain

{
version 0.8
folder0	[ 0	locks=0 ]	(	1	1	)
group	[ 0	locks=0 ]	(	origprims_subdivide	)
grouptype	[ 0	locks=0 ]	(	"guess"	)
class	[ 0	locks=0 ]	(	"point"	)
vex_numcount	[ 0	locks=0 ]	(	10	)
vex_threadjobsize	[ 0	locks=0 ]	(	1024	)
snippet	[ 0	locks=0 ]	(	"int divs = 2;
float s = 1.0 / divs;

vector origin = 0;

int pts [ ] = neighbours ( 0, @ptnum );
foreach ( int pt; pts )
{
    if ( @ptnum < pt && inedgegroup ( 0, \"origprimedges_subdivide\", @ptnum, pt ) )
    {
        for ( int i = 1; i < divs; ++i )
        {
            int newpt = addpoint ( 0, origin );
            setpointgroup ( 0, \"edgepts_subdivide\", newpt, 1 );
            
            string edgepts = sprintf ( \"%s %s\", min ( @ptnum, pt ), max ( @ptnum, pt ) );
            setpointattrib ( 0, \"edgepts\", newpt, edgepts );
            setpointattrib ( 0, \"isclosed\", newpt, i@isclosed );
            
            float w = s * i;
            int indices [ ] = array ( @ptnum, pt );
            float weights [ ] = array ( 1 - w, w );
            setpointattrib ( 0, \"sourcept_indices\", newpt, indices );
            setpointattrib ( 0, \"sourcept_weights\", newpt, weights );
        }
    }
}"	)
exportlist	[ 0	locks=0 ]	(	*	)
vex_strict	[ 0	locks=0 ]	(	"off"	)
autobind	[ 0	locks=0 ]	(	"on"	)
bindings	[ 0	locks=0 ]	(	0	)
groupautobind	[ 0	locks=0 ]	(	"on"	)
groupbindings	[ 0	locks=0 ]	(	0	)
vex_cwdpath	[ 0	locks=0 ]	(	.	)
vex_outputmask	[ 0	locks=0 ]	(	*	)
vex_updatenmls	[ 0	locks=0 ]	(	"off"	)
vex_matchattrib	[ 0	locks=0 ]	(	id	)
vex_inplace	[ 0	locks=0 ]	(	"off"	)
vex_selectiongroup	[ 0	locks=0 ]	(	""	)
vex_precision	[ 0	locks=0 ]	(	auto	)
folder1	[ 0	locks=0 ]	(	0	0	)
}

