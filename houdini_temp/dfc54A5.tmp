snippet	[ 0	locks=0 ]	(	"int isclosed = 1;
int primcount = 0;

int prims [ ] = pointprims ( 0, @ptnum );
foreach ( int pr; prims )
{
    isclosed = primintrinsic ( 0, \"closed\", pr );
    if ( !isclosed )
        break;
}
i@isclosed = isclosed;

foreach ( int pr; prims )
    if ( inprimgroup ( 0, \"origprims_subdivide\", pr ) )
        primcount += 1;

int isboundarypt = inpointgroup ( 0, \"origprimborderpts_subdivide\", @ptnum );
i@isboundarypt = isboundarypt;

if ( isclosed && primcount < 2 )
    i@group_origpts_subdivide = 0;
    
if ( !isclosed && neighbourcount ( 0, @ptnum ) < 2 )
    i@group_origpts_subdivide = 0;"	)
exportlist	[ 0	locks=0 ]	(	*	)
vex_strict	[ 0	locks=0 ]	(	"off"	)
autobind	[ 0	locks=0 ]	(	"on"	)
