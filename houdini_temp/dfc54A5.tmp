--HOUDINIMIMEBOUNDARY0xD3ADD339-0x00000F49-0x56B122C9-0x00000001HOUDINIMIMEBOUNDARY
Content-Disposition: attachment; filename="hdaroot/create_new_primitives.parm"
Content-Type: text/plain

{
version 0.8
folder0	[ 0	locks=0 ]	(	1	1	)
group	[ 0	locks=0 ]	(	origprims_subdivide	)
grouptype	[ 0	locks=0 ]	(	"guess"	)
class	[ 0	locks=0 ]	(	"primitive"	)
vex_numcount	[ 0	locks=0 ]	(	10	)
vex_threadjobsize	[ 0	locks=0 ]	(	1024	)
snippet	[ 0	locks=0 ]	(	"int pts [ ] = primpoints ( 0, @primnum );
int vtx [ ] = primvertices ( 0, @primnum );

int count = len ( pts );

int isclosed = primintrinsic ( 0, \"closed\", @primnum );
if ( isclosed )
{
    append ( pts, pts [ 0 ] );
    append ( vtx, vtx [ 0 ] );
}
else
    count -= 1;

int centerpt = findattribval ( 0, \"point\", \"primcenterpt\", @primnum );
int edgepts [ ] = { };

for ( int i = 0; i < count; ++i )
{
    string sedgepts = sprintf ( \"%s %s\", min ( pts [ i ], pts [ i + 1 ] ), max ( pts [ i ], pts [ i + 1 ] ) );
    int index = findattribval ( 0, \"point\", \"edgepts\", sedgepts );
    append ( edgepts, index );
}

if ( isclosed )
    removeindex ( pts, -1 );

string typename = \"poly\";
if ( !isclosed )
    typename = \"polyline\";

removeprim ( 0, @primnum, 0 );

int newpts [ ] = { };
int vertexindices [ ] = { };

int primcount = count;
if  ( !isclosed )
    primcount = 1;
    
int pr = -1;
int newvertices [ ] = { };
for ( int i = 0; i < primcount; ++i )
{
    int lastindex = ( i + count - 1 ) % count;
    if ( isclosed )
    {
        int pt0 = pts [ i ];
        int pt1 = edgepts [ i ];
        int pt2 = centerpt;
        int pt3 = edgepts [ lastindex ];
        newpts = array ( pt0, pt1, pt2, pt3 );
    }
    else
    {
        int edgecount = len ( edgepts );
        for ( int f = 0; f <= count; ++f )
        {
            append ( newpts, pts [ f ] );
            append ( vertexindices, vtx [ f ] );
            if ( f < edgecount )
            {
                append ( newpts, edgepts [ f ] );
                append ( vertexindices, -1 );
            }
        }
    }
    
    addprim ( pr, 0, typename, newpts, newvertices );
    
    int primindices [ ] = array ( @primnum );
    float primweights [ ] = array ( 1.0 );
    
    setprimattrib ( 0, \"sourceprim_index\", pr, @primnum );
    setprimattrib ( 0, \"sourceprim_indices\", pr, primindices );
    setprimattrib ( 0, \"sourceprim_weights\", pr, primweights );
    setprimgroup ( 0, \"origprims_subdivide\", pr, 1 );
    
    int sourcevtx_indices [ ] = { };
    float sourcevtx_weights [ ] = { };
    int sourcevtx_groupindices [ ] = { };
    
    if ( isclosed )
    {
        for ( int f = 0; f < 4; ++f )
        {
            if ( f == 0 ) // origpts
            {
                sourcevtx_indices = array ( vtx [ i ] );
                sourcevtx_weights = array ( 1.0 );
                sourcevtx_groupindices = sourcevtx_indices;
            }
            else if ( f == 1 ) // edgepts
            {
                sourcevtx_indices = array ( vtx [ i ], vtx [ i + 1 ] );
                sourcevtx_weights = array ( 0.5, 0.5 );
                sourcevtx_groupindices = array ( -1, -1 );
            }
            else if ( f == 2 ) // centerpt
            {
                sourcevtx_indices = vtx [ : -1 ];
                sourcevtx_weights = point ( 0, \"sourcept_weights\", centerpt );
                sourcevtx_groupindices = array ( -1, -1 );
            }
            else if ( f == 3 ) // edgepts
            {
                sourcevtx_indices = array ( vtx [ lastindex ], vtx [ lastindex + 1 ] );
                sourcevtx_weights = array ( 0.5, 0.5 );
                sourcevtx_groupindices = array ( -1, -1 );
            }
            
            setvertexattrib ( 0, \"sourcevtx_indices\", newvertices [ f ], -1, sourcevtx_indices );
            setvertexattrib ( 0, \"sourcevtx_weights\", newvertices [ f ], -1, sourcevtx_weights );
            setvertexattrib ( 0, \"sourcevtx_groupindices\", newvertices [ f ], -1, sourcevtx_groupindices );
            setvertexgroup ( 0, \"newvtx_subdivide\", newvertices [ f ], -1, 1 );
        }
    }
    else
    {
        int index = 0;
        int vtxcount = len ( newvertices );
        for ( int f = 0; f < vtxcount; ++f )
        {
            int vertexindex = vertexindices [ f ];
            if ( vertexindex != -1 )
            {
                sourcevtx_indices = array ( vertexindex );
                sourcevtx_weights = array ( 1.0 );
                sourcevtx_groupindices = sourcevtx_indices;
            }
            else
            {
                sourcevtx_indices = array ( vtx [ index ], vtx [ index + 1 ] );
                sourcevtx_weights = array ( 0.5, 0.5 );
                sourcevtx_groupindices = array ( -1, -1 );
                
                index += 1;
            }
            
            setvertexattrib ( 0, \"sourcevtx_indices\", newvertices [ f ], -1, sourcevtx_indices );
            setvertexattrib ( 0, \"sourcevtx_weights\", newvertices [ f ], -1, sourcevtx_weights );
            setvertexattrib ( 0, \"sourcevtx_groupindices\", newvertices [ f ], -1, sourcevtx_groupindices );
            setvertexgroup ( 0, \"newvtx_subdivide\", newvertices [ f ], -1, 1 );
        }
    }
}"	)
